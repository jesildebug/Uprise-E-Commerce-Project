                    <a href="/cart">
                        <i class="fa fa-shopping-cart" style="color: black; margin-left: 26px;"></i>
                        
                    </a>
                </div>


                    <% if(!login) { %>
                        <div class="user-access">
                            <a href="/signuppage">Register</a>
                            <a href="/login" class="in">Sign in</a>
                        </div>
                        <% } %>

                            <% if(login) { %>
                                <div class="user-access">
                                    <a href="/logout" class="in">Log Out</a>
                                </div>
                                <% } %>


                                    <nav class="main-menu mobile-menu">
                                        <ul>
                                           
                                            <li>
                                                <a class="active" href="/homepage">Home</a>
                                            </li>
                                            <li>
                                                <a href="/products">Shop</a>
